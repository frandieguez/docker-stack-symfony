{% extends 'base.html.twig' %}

{% block body %}
<div id="wrapper">
    <div id="container">
        <h1>Pets</h1>
        <ul>
            {% for pet in pets %}
                <li><strong>{{ pet.name|e }}</strong> ({{ pet.species}}) <small>owned by {{ pet.owner}}</small></li>
            {% endfor %}
        </ul>
    </div>
</div>
{% endblock %}